Load "examples/natural_numbers.tt".

Definition pair := λ (a b i: N) => if_then_else i b a.

Definition iter := λ (f: N -> N) => λ (n: N) => λ (x: N) => ind(
    λ n => N,
    x,
    λ n m => f m,
    n
).

Definition A := λ (m: N) => ind(
    λ n => N -> N,
    λ n => n + 1,
    λ m Am => λ (n: N) => iter Am (n+1) 1,
    m
).
